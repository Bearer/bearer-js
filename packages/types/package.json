{
  "name": "@bearer/types",
  "version": "0.105.0",
  "description": "Provide typing for Bearer packages",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "Bearer/bearer",
  "homepage": "https://github.com/Bearer/bearer/packages/types#readme",
  "author": "Bearer Team<engineering@bearer.sh>",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "build": "tsc",
    "start": "yarn build -w",
    "test": "jest --colors",
    "test:ci": "jest --coverage --runInBand --colors",
    "prepare": "yarn clean && yarn build",
    "prepack": "yarn clean && NODE_ENV=production yarn build"
  },
  "keywords": [
    "types",
    "typescript",
    "bearer"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
