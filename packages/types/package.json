{
  "name": "@bearer/types",
  "version": "0.90.0",
  "description": "Provide typing for Bearer packages",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "build": "tsc",
    "start": "yarn build -w",
    "test": "jest",
    "test:ci": "jest --coverage --runInBand",
    "prepare": "yarn clean && yarn build",
    "prepack": "yarn clean && NODE_ENV=production yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bearer/bearer.git"
  },
  "keywords": [
    "types",
    "typescript",
    "bearer"
  ],
  "author": "Bearer Team<engineering@bearer.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Bearer/bearer/issues"
  },
  "homepage": "https://github.com/Bearer/bearer#readme",
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
