{
  "name": "{{componentTagName}}",
  "version": "0.0.1",
  "description": "{{componentName}}",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "module": "dist/esm/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "scripts": {
    "build": "stencil build --es5",
    "dev": "stencil build --dev --watch --serve --es5",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@bearer/core": "{{bearerTagVersion}}",
    "@bearer/intents": "{{bearerTagVersion}}",
    "@bearer/ui": "{{bearerTagVersion}}",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@bearer/cli": "{{bearerTagVersion}}",
    "@types/jest": "latest",
    "@types/node": "latest",
    "jest": "latest"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
