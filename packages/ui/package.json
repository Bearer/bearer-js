    "prepare": "BUILD=distribution yarn build",
    "prepublishOnly": "BUILD=distribution yarn build"
  },
  "dependencies": {
    "@bearer/core": "^0.66.0",
    "@stencil/core": "0.11.2",
    "@stencil/sass": "^0.1.0",
