{
  "name": "@bearer/ui",
  "version": "0.66.0",
  "description": "Bearer UI helper",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/components.d.ts",
  "collection": "lib/collection/collection-manifest.json",
  "files": [
    "lib/"
