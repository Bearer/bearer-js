{
  "name": "@bearer/core",
  "version": "0.93.0",
  "description": "Bearer Core",
  "main": "lib/main.js",
  "types": "lib/index.d.ts",
  "module": "lib/main.es.js",
  "browser": "lib/main.browser.js",
  "author": "Bearer Team<engineering@bearer.sh>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "start": "yarn build -w",
    "build": "rollup -c",
    "prepare": "yarn clean && rollup -c",
    "prepack": "yarn clean && BUILD=distribution NODE_ENV=production yarn build",
    "test": "jest",
    "test:ci": "jest --coverage --runInBand"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "github:Bearer/bearer"
  },
  "devDependencies": {
    "@types/fbemitter": "^2.0.32",
    "@types/jest": "^23.3.13",
    "@types/lodash.get": "^4.4.4",
    "@types/lodash.merge": "^4.6.4",
    "@types/lodash.template": "^4.4.4",
    "@types/node": "10.12.18",
    "dotenv": "^6.2.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^1.7.4",
    "per-env": "^1.0.2",
    "rollup": "^0.67.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-html-entry": "^0.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript2": "^0.17.2",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1"
  },
  "dependencies": {
    "@bearer/types": "^0.93.0",
    "@stencil/core": "0.11.2",
    "fbemitter": "^2.1.1",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.1",
    "lodash.template": "^4.4.0",
    "post-robot": "^8.0.26",
    "rc": "^1.2.8",
    "rollup-plugin-env": "^0.21.4",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-strip": "^1.2.0",
    "typescript": "^3.2.4"
  },
  "gitHead": "cb2654b19000e2ad6f31a0e4ed715a6298c8df2d"
}
