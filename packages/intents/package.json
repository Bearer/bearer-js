{
  "name": "@bearer/intents",
  "version": "0.82.0",
  "description": "Bearer Intents",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Bearer Team<engineering@bearer.sh>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "build": "tsc -p tsconfig.json",
    "start": "yarn clean && yarn build -w",
    "prepare": "yarn clean && yarn build",
    "prepack": "NODE_ENV=production yarn build",
    "test": "jest",
    "test:ci": "jest --coverage --runInBand"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "github:BearerSH/bearer"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.6",
    "jest": "^23.6.0",
    "jest-mock-axios": "^2.1.11",
    "ts-jest": "^23.10.4"
  },
  "gitHead": "cb2654b19000e2ad6f31a0e4ed715a6298c8df2d"
}
